{
  "task": "flat norm dirac less than 2",
  "output_root": "out/loss_balancing",
  "exp_name": "FN of 2 Diracs",
  "seed": 0,
  "model": {
    "pretrained_best_path": null,
    "name": "dual_fc_flat",
    "activation": "maxmin",
    "linear": {
      "type": "spectral_normal",
      "safe_scaling": true,
      "power_iters": 2,
      "bjorck_beta": 0.5,
      "bjorck_iter": 20,
      "bjorck_order": 1,
      "bias": true
    },
    "layers": [
      128,
      128,
      1
    ],
    "groupings": [
      2,
      2,
      1
    ],
    "l_constant": 1,
    "upper_bound":1,
    "lambda_loss": 10,

    "per_epoch_proj": {
      "turned_on": false,
      "every_n_epochs": 100000000,
      "type": "l_2",
      "bjorck_beta": 0.5,
      "bjorck_iter": 20,
      "bjorck_order": 1,
      "reset_optimizer": false
    },

    "per_update_proj": {
      "turned_on": false,
      "type": "l_2",
      "bjorck_beta": 0.5,
      "bjorck_iter": 12,
      "bjorck_order": 1
    }
  },
  "optim": {
    "optimizer": "aggmo",
    "lr_schedule": {
      "name": "step",
      "lr_init": 0.01,
      "lr_decay": 0.9,
      "milestones": [
        32,
        64
      ],
      "last_epoch": -1
    },
    "epochs": 30,
    "epoch_len": 16,
    "momentum": 0.9,
    "betas": [
      0.0,
      0.9,
      0.99
    ],
    "wdecay": 0.0,
    "criterion": {
      "tag": "loss",
      "minmax": "min"
    },
    "patience": 250,
    "max_grad_norm": 10
  },
  "distrib1": {
    "name": "Dirac",
    "filepath": "lnets/tasks/dualnets/distrib/Dirac.py",
    "dim": 1,
    "x": [
      0.0
    ],
    "sample_size": 1,
    "test_sample_size": 1
  },
  "distrib2": {
    "name": "Dirac",
    "filepath": "lnets/tasks/dualnets/distrib/Dirac.py",
    "dim": 1,
    "x": [
      10.0
    ],
    "sample_size": 1,
    "test_sample_size": 1
  },
  "visualize_2d": {
    "xrange": [
      -4.1,
      4.1
    ],
    "yrange": [
      -1.1,
      1.1
    ],
    "step": 0.02,
    "fig_types": [
      "contour"
    ],
    "elev": [
      0,
      60,
      90
    ],
    "azim": [
      0
    ]
  },
  "visualize_1d": {
    "xrange": [
      -1.2,
      1.2
    ],
    "step": 0.02
  },
  "visualize_logs": {
    "losses": true,
    "weight_orthognality": true,
    "weight_orthognality_yrange": [0, 5]
  },
  "logging": {
    "report_freq": 1,
    "save_model": false,
    "save_best": true,
    "check_orthogonality": true
  },
  "cuda": false,
  "visualize": true
}